{
  "name": "appexample",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "lint": "eslint . --fix",
    "prepare": "husky",
    "husky:setup": "npx husky init && echo \"lint-staged\" > .husky/pre-commit"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass-embedded": "^1.89.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.8.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "vite": "^6.3.5"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
